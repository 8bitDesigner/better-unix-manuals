{
  "manifest_version": 2,
  "name": "better-unix-manuals",
  "description": "Basic styling for the GNU software manuals",
  "version": "1.1.0",
  "content_scripts": [
    {
      "js": [
        "js/jquery-2.1.3.min.js",
        "js/highlight.min.js",
        "js/index.js"
      ],
      "css": [
        "css/default.min.css",
        "css/index.css"
      ],
      "matches": [
        "*://www.gnu.org/software/*/manual/*.html",
        "*://www.gnu.org/software/*/manual/html_node/*.html",
        "*://tldp.org/LDP/*/html/*.html",
        "*://www.gnupg.org/howtos/*/*.html",
        "*://www.gnupg.org/gph/*/*.html"
      ]
    }
  ]
}